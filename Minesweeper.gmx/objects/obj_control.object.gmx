            <string>//Draw Tiles
for (var xx = 0; xx &lt; ds_grid_width(grid); xx++){
    for (var yy = 0; yy &lt; ds_grid_height(grid); yy++){
        draw_sprite(spr_floorTiles,grid[#xx,yy],xx*TILE_WIDTH,yy*TILE_HEIGHT);
    }
}

var mx = (mouse_x div 32 * 32)/TILE_WIDTH;
var my = (mouse_y div 32 * 32)/TILE_HEIGHT;

if (mouse_check_button_pressed(mb_left) &amp;&amp; position_meeting(mouse_x,mouse_y,obj_tile)){
    if (!generated){ 
        grid = generateGrid(mx,my,ds_grid_width(grid),ds_grid_height(grid),mines);
    }
    if (collision_point(mx*TILE_WIDTH,my*TILE_HEIGHT,obj_tile,false,true).image_index != 1) position_destroy(mouse_x,mouse_y);
    else exit;
    if (grid[#mx,my] == TILE_MINE){ 
    
        for (var xx = 0; xx &lt; ds_grid_width(grid); xx++){
            for (var yy = 0; yy &lt; ds_grid_height(grid); yy++){
                if (grid[#xx,yy] == TILE_MINE) position_destroy(xx*TILE_WIDTH,yy*TILE_HEIGHT);
            }
        }
        
        gameOver = true;
    }
    if (grid[#mx,my] == TILE_EMPTY &amp;&amp; generated) reveal(mx,my); 
}

if (instance_number(obj_tile) == mines &amp;&amp; global.flags &gt;= (mines/3)*2){ 
    if (gameOver = false) show_message("You Win");
    gameOver = true;
}
